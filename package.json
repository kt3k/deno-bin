  "bugs": {
    "url": "https://github.com/kt3k/deno-bin/issues"
  },
  "homepage": "https://github.com/kt3k/deno-bin#readme",
  "dependencies": {
    "adm-zip": "^0.5.4",
    "follow-redirects": "^1.10.0"
  }
}
