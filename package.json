{
  "name": "deno-bin",
  "version": "1.19.1",
  "description": "CLI wrapper for Deno, a secure runtime for JavaScript and TypeScript",
  "main": "index.js",
  "files": [
    "install.js",
    "bin"
  ],
  "bin": {
    "deno": "./bin/deno",
